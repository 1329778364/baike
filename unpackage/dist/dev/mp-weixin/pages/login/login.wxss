.phone-head-box {
  position: relative;
}
.phone-head-box .phone-head {
  position: absolute;
  height: 100%;
  left: 0;
  top: 0;
  width: 100rpx;
}
.phone-input-box {
  padding-left: 100rpx;
}
.login-input {
  padding-right: 150rpx;
}
.login-input-box {
  position: relative;
}
.login-input-box .forget {
  position: absolute;
  height: 100%;
  top: 0;
  right: 0;
  width: 150rpx;
}
.logging-padding {
  padding: 20rpx 0;
}
.other-login-title {
  position: relative;
}
.other-login-title::before .other-login-title::after {
  position: absolute;
  height: 1rpx;
  width: 100rpx;
  top: 50%;
}
.other-login-title::before {
  left: 25%;
}
.other-login-title::after {
  right: 25%;
}
.login-head {
  width: 100%;
}
.guanbi {
  position: fixed;
  left: 30rpx;
  top: 60rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #332F0A;
}

