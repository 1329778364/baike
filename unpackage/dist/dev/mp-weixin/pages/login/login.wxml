<view><uni-status-bar vue-id="1" bgcolor="#ffe933" bind:__l="__l"></uni-status-bar><view data-event-opts="{{[['tap',[['navBack',['$event']]]]]}}" class="icon iconfont icon-guanbi guanbi" bindtap="__e"></view><image class="login-head" src="../../static/loginhead.png" mode="widthFix"></image><view class="page"><block wx:if="{{status}}"><input class="uni-input common-input" type="text" value="{{username}}" placeholder="昵称/手机号/邮箱" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" bindinput="__e"/><view class="login-input-box"><input class="uni-input common-input login-input" type="text" value="{{password}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" bindinput="__e"/><view class="forget u-f-ajc bgc-yellow">忘记密码?</view></view></block><block wx:else><view class="phone-head-box"><view class="phone-head u-f-ajc">+86</view><input class="uni-input common-input phone-input-box" type="text" value="{{phone}}" placeholder="输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" bindinput="__e"/></view><view class="login-input-box"><input class="uni-input common-input login-input" type="text" value="{{checknum}}" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','checknum','$event',[]]]]]]}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getCheckNum',['$event']]]]]}}" class="forget u-f-ajc" bindtap="__e"><view class="u-f-ajc bgc-gray" style="border-radius:10rpx;padding:5rpx;">{{!codetime?'获取验证码':codetime+'s'}}</view></view></view></block><view class="u-f-dc u-f-ajc"><button class="{{['btn-normal bgc-yellow ',disable?'btn-disable':'']}}" loading="{{loading}}" disabled="{{disable}}" type="primary" data-event-opts="{{[['tap',[['submmit',['$event']]]]]}}" bindtap="__e">登录</button></view><view data-event-opts="{{[['tap',[['changeStatus',['$event']]]]]}}" class="u-f-ajc font-size-lg  logging-padding " bindtap="__e">{{''+(status?'验证码登录':'账号登录')}}<view class="icon iconfont icon-jinru"></view></view><view class="u-f-ajc other-login-title logging-padding font-color-gray">第三方登录</view><other-login vue-id="2" noback="{{false}}" bind:__l="__l"></other-login><view class="login-rule u-f-ajc logging-padding font-color-gray">注册即代表您同意<view>《xxx协议》</view></view></view></view>