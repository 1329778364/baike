<view class="navigation-bar" style="{{'height:'+(height)+';'}}"><view class="navigation-bar-fixed" style="{{'height:'+(height)+';'+('background:'+(!config.transparent&&!config.linear?config.bgcolor:'transparent')+';')}}"><block wx:if="{{$root.g0!=-1}}"><view class="{{[config.type==1?'navigation-bar-capsule':'navigation-bar-textbox']}}" style="{{'top:'+(marginTop)+';'}}"><view class="button-action" hover-class="button-action-active" data-event-opts="{{[['tap',[['clickleft',['$event']]]]]}}" bindtap="__e"><image src="{{config.leftimage}}" mode></image></view><view class="button-v-line"></view><block wx:if="{{!config.share}}"><view class="button-action" hover-class="button-action-active" data-event-opts="{{[['tap',[['clickright',['$event']]]]]}}" bindtap="__e"><image src="{{config.rightimage}}" mode></image></view></block><block wx:if="{{config.share}}"><view class="button-action" hover-class="button-action-active"><button class="button_clear" open-type="share" hover-class="none"><image src="../../static/icon/navbar/share_.png" mode></image></button></view></block></view></block><block wx:if="{{$root.g1!=-1}}"><view data-event-opts="{{[['tap',[['conduct_',['$event']]]]]}}" class="{{[config.type==3?'navigation-bar-custom-capsule':'navigation-bar-custom-textbox']}}" style="{{'top:'+(marginTop)+';'}}" bindtap="__e"><block wx:if="{{config.menuIcon}}"><image class="custom-icon" src="{{config.menuIcon}}"></image></block><text class="custom-describe">{{config.menuText}}</text></view></block><view class="navigation-bar-title" style="{{('margin-top:'+marginTop+';color:'+config.fontcolor)}}"><block wx:if="{{$slots.middle}}"><slot name="middle"></slot></block><block wx:else>{{config.title}}</block></view><block wx:if="{{config.linear}}"><view class="navigation-bar-linear" style="{{'height:'+(height)+';'+('background:'+(config.bgcolor)+';')+('opacity:'+(scrollTop/scrollMaxHeight)+';')}}"></view></block></view></view>